<div>
  <% @search_list.each do |record| %>  
    <% if record.class.name.equal? Service.name %>

      <p> <%= link_to record.service_name, services_path%> </p>

    <% elsif record.class.name.equal? Visit.name %>

      <p>Visit with <%= link_to record.service.service_name, client_visits_path(current_user) %> </p>

    <% end %>
  <% end %>
</div> 